<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
     <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <link href="~/lib/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link
        href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
        rel="stylesheet">
    <link href="~/css/sb-admin-2.min.css" rel="stylesheet">
</head>
<body id="page-top">
    <!-- Page Wrapper -->
    <div id="wrapper">
      
     @Html.Partial("~/Views/Back-end/Component/SideBar.cshtml")
     @RenderBody()
     @Html.Partial("~/Views/Back-end/Component/Footer.cshtml");}  
     </div>
     <!-- Bootstrap core JavaScript-->
    <script src="~/lib/jquery/jquery.min.js"></script>
    <script src="~/lib/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Core plugin JavaScript-->
    <script src="~/lib/jquery-easing/jquery.easing.min.js"></script>

    <!-- Custom scripts for all pages-->
    <script src="~/lib/sb-admin-2.min.js"></script>

    <!-- Page level plugins -->
    <script src="~/lib/chart.js/Chart.min.js"></script>

    <!-- Page level custom scripts -->
    <script src="~/lib/demo/chart-area-demo.js"></script>
    <script src="~/lib/demo/chart-pie-demo.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
